         13                           FUNCTION I4FLAG(INT4,IGET)
         14                           IMPLICIT INTEGER*4(I-N)
         15                           PARAMETER ( MBASE = 10 )
         16                           DIMENSION IBASE(MBASE)
         17                     C
         18                     C
         19                     C      RETURN THE DECIMAL NUMBER RESIDING IN A SPECIFIED POSITION OF
         20                     C     A PASSED INTEGER
         21                     C         ( 2-D , 3-D CALCULATION & GRAPHICS )
         22                     C
         23                     C
         24                     C     NOTE 1 ; IGET MUST BE UNDER OR EQUAL TO 10 (AND GREATER THAN 0).
         25                     C             IF THE ABOVE CONDITION IS NOT MET, AN VALUE OF -1 WILL
         26                     C             BE RETURNED AS FUNCTION RETURN VALUE.
         27                     C
         28                     C     NOTE 2 ; IF NO NUMBER RESIDES IN THE SPECIFIED POSITION OF THE
         29                     C             PASSED INTEGER, AN VALUE OF 0 WILL BE RETURNED AS
         30                     C             FUNCTION RETURN VALUE.
         31                     C
         32                     C
         33                     C     ARGUMENT LISTINGS
         34                     C       (1) INPUT
         35                     C          INT4        ; 4-BYTE INTEGER NUMBER
         36                     C          IGET        ; SPECIFIES THE POSITION FROM THE LOWEST
         37                     C
         38                     C       (5) RETURN VALUE
         39                     C          I4FLAG      ; DECIMAL NUMBER RESIDING IN THE SPECIFIED
         40                     C                       POSITION OF THE PASSED INTEGER NUMBER
         41                     C
         42                     C
         43                           DATA IBASE /          1 ,
         44                          &                     10 ,
         45                          &                    100 ,
         46                          &                   1000 ,
         47                          &                  10000 ,
         48                          &                 100000 ,
         49                          &                1000000 ,
         50                          &               10000000 ,
         51                          &              100000000 ,
         52                          &             1000000000 /
         53                     C
         54     1                     IF(IGET.LE.0 .OR. IGET.GE.11) THEN
         55     1                         I4FLAG = -1
         56     1                         RETURN
         57     1                     ENDIF
         58                     C
         59     1                     IF(INT4.GE.0) THEN
         60     1                         IDUM =  INT4
         61     1                     ELSE
         62     1                         IDUM = -INT4
         63     1                     ENDIF
         64                     C
         65                           I4FLAG = MOD(IDUM/IBASE(IGET),10)
         66                     C
         67                     C
         68                           RETURN
         69                           END

