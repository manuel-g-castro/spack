         13                           SUBROUTINE FLUX2D(IMODE,DT,AU,AV,N,DELTA,LEB,DSB,DCOSB,SNIB,NEB,
         14                          *                  FLUXB)
         15                           IMPLICIT REAL*8(A-H,O-Z)
         16                           DIMENSION AU(N,*),AV(N,*),DELTA(*),
         17                          1          LEB(NEB),DSB(NEB),DCOSB(2,NEB),SNIB(N,NEB),FLUXB(NEB)
         18                     C
         19                     C
         20                     C      CALCULATE HALF VOLUME FLOW LEAVING OUTFLOW BOUNDARY SIDES
         21                     C     IN ONE TIME INTEGRATION
         22                     C         ( 2-D CALCULATION )
         23                     C
         24                     C
         25                     C     ARGUMENT LISTINGS
         26                     C       (1) INPUT
         27                     C          DT          ; TIME INCREMENT
         28                     C          AU (I,IE)   ; ELEMENT VECTOR OF U(IP)
         29                     C          AV (I,IE)   ; ELEMENT VECTOR OF V(IP)
         30                     C          N           ; NUMBER OF NODES ASSIGNED TO ONE ELEMENT ( N=4 )
         31                     C
         32                     C          LEB     (IB); ELEMENT NUMBERS FACING ON BOUNDARIES
         33                     C          DSB     (IB); LENGTH OF SIDES FACING ON BOUNDARIES
         34                     C          DCOSB (I,IB); DIRECTIONAL COSINE          OF BOUNDARY SIDES
         35                     C          SNIB  (I,IB); SHAPE FUNCTION AT MID POINT OF BOUNDARY SIDES
         36                     C          NEB         ; NUMBER OF SIDES FACING ON BOUNDARIES
         37                     C
         38                     C       (2) OUTPUT
