                              <<<      SIMD&FP   : SPILL 0  FILL 2
                              <<<      SCALABLE  : SPILL 0  FILL 0
                              <<<      PREDICATE : SPILL 0  FILL 0
                              <<< Loop-information  End >>>
         41     1                     DO 100 INL = 1 , NMAX
         42     1                         F    = RT**N-SIG/A*RT+SIG/A-1.D0
         43     1                         DFDR = N*RT**(N-1)-SIG/A
         44     1                         DR   =-F/DFDR
         45     1                         RT   = RT + DR
         46     1                         SIGE = A*(RT**N-1.D0)/(RT-1.D0)
         47     1                         ERR  = (SIGE-SIG)/SIG
