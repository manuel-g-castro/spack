         13                           SUBROUTINE NEWTON(A,SIG,N,RTINIT,EPS,NMAX,RT,IRN,NRN,ERR)
         14                           IMPLICIT REAL*4 (A-H,O-Z)
         15                     C
         16                     C
         17                     C      CALCULATE ISO-RATIO BY NEWTON-LAPSON METHOD
         18                     C         ( 2-D , 3-D GRAPHICS )
         19                     C
         20                     C
         21                     C     ARGUMENT LISTINGS
         22                     C       (1) INPUT
         23                     C          A            ; INITIAL TERM
         24                     C          SIG          ; SIGMA
         25                     C          N            ; NUMBER OF TERMS
         26                     C          RTINIT       ; INITIAL VALUE OF ISO-RATIO
         27                     C          EPS          ; CONVERGENCE JUDGING PARAMETER
         28                     C          NMAX         ; MAX. ITERATION NO.
         29                     C
         30                     C       (2) OUTPUT
         31                     C          RT           ; ISO-RATIO
         32                     C          IRN          ; RETURN CODE ( IRN = 1 CONVERGED , = 0 NOT )
         33                     C          NRN          ; CALCULATION ITERATED NO.
         34                     C          ERR          ; RESIDUAL ERROR
         35                     C
         36                     C
         37                     C
         38                           IRN = 0
         39                           NRN = NMAX
         40                           RT  = RTINIT
