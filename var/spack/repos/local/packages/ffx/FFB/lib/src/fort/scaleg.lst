         13                           SUBROUTINE SCALEG(X,Y,NP,RNGX,RNGY,RATIO,XMIN,XMAX,YMIN,YMAX,SFC)
         14                           IMPLICIT REAL*4(A-H,O-Z)
         15                           DIMENSION X(NP),Y(NP)
         16                     C
         17                     C
         18                     C      SCALE 2-DIMENSIONAL FIELD
         19                     C         ( 2-D GRAPHICS )
         20                     C
         21                     C
         22                     C     ARGUMENT LISTINGS
         23                     C       (1) INPUT
         24                     C          X(IP)       ; X-DIR. COORDINATE
         25                     C          Y(IP)       ; Y-DIR. COORDINATE
         26                     C          NP          ; NUMBER OF TOTAL NODES
         27                     C          RNGX        ; X-DIR. GRAPHIC RANGE SPECIFIED
         28                     C          RNGY        ; Y-DIR. GRAPHIC RANGE SPECIFIED
         29                     C          RATIO       ; SCALING COEFFICIENT
         30                     C                RATIO < 1.0 --- UNDER SCALING
         31                     C                RATIO > 1.0 --- OVER  SCALING
         32                     C
         33                     C       (2) OUTPUT
         34                     C          XMIN        ; MINIMUM X-VALUE
         35                     C          XMAX        ; MAXIMUM X-VALUE
         36                     C          YMIN        ; MINIMUM Y-VALUE
         37                     C          YMAX        ; MAXIMUM Y-VALUE
         38                     C          SFC         ; SCALING FACTOR
