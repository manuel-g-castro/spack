         40                     C
         41                           CALL FINDMM(X,NP,OXMIN,OXMAX)
         42                           CALL FINDMM(Y,NP,OYMIN,OYMAX)
         43                     C
         44                           XMIN = OXMIN-0.5*(1.0-RATIO)/RATIO*(OXMAX-OXMIN)
         45                           XMAX = OXMAX+0.5*(1.0-RATIO)/RATIO*(OXMAX-OXMIN)
         46                           YMIN = OYMIN-0.5*(1.0-RATIO)/RATIO*(OYMAX-OYMIN)
         47                           YMAX = OYMAX+0.5*(1.0-RATIO)/RATIO*(OYMAX-OYMIN)
         48                     C
         49                           SFC = AMIN1(RNGX/(XMAX-XMIN) , RNGY/(YMAX-YMIN))
         50                     C
         51                     C
