         13                           SUBROUTINE FINDMM(S,NP,SMIN,SMAX)
         14                           IMPLICIT REAL*4(A-H,O-Z)
         15                           DIMENSION S(NP)
         16                     C
         17                     C
         18                     C      FIND MAXIMUM AND MINIMUM VALUE AMONG GIVEN ARRAY ELEMENTS
         19                     C         ( 2-D , 3-D GRAPHICS )
         20                     C
         21                     C
         22                     C     ARGUMENT LISTINGS
         23                     C       (1) INPUT
         24                     C          S(IP)       ; GIVEN ARRAY
         25                     C          NP          ; NUMBER OF ARRAY ELEMENTS
         26                     C       (2) OUTPUT
         27                     C          SMIN        ; MINIMUM VALUE OF AMONG THE ARRAY ELEMENTS
         28                     C          SMAX        ; MAXIMUM VALUE OF AMONG THE ARRAY ELEMENTS
         29                     C
         30                     C
         31     1   s   2s            DO 100 IP = 1 , NP
         32     2   m   2m                IF(IP.EQ.1 .OR. S(IP).LE.SMIN) THEN
         33     2   s   2s                   SMIN = S(IP)
         34     2   p   2v                ENDIF
         35     2   m   2m                IF(IP.EQ.1 .OR. S(IP).GE.SMAX) THEN
         36     2   s   2s                   SMAX = S(IP)
         37     2   p   2v                ENDIF
         38     1   p   2v        100 CONTINUE
         39                     C
         40                     C
         41                           RETURN
         42                           END

