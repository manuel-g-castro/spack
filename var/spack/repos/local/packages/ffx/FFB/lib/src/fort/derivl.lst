         13                           SUBROUTINE DERIVL(G,E,XE,YE,N,DNX,DNY)
         14                           IMPLICIT REAL*4(A-H,O-Z)
         15                           DIMENSION XE(N),YE(N),DNX(N),DNY(N)
         16                     C
         17                           DIMENSION DNG(4),DNE(4)
         18                     C
         19                     C
         20                     C      CALCULATE X,Y DERIVERTIVES OF SHAPE FUNCTION
         21                     C     AT A SPECIFIED POINT IN AN ELEMENT
         22                     C         ( 2-D GRAPHICS )
         23                     C
         24                     C
         25                     C     NOTE ; 1. CALCULATION IS CARRIED OUT AT A GIVEN LOCATION(G,E)
         26                     C              IN ONE ELEMENT.
         27                     C
         28                     C
         29                     C     ARGUMENT LISTINGS
         30                     C       (1) INPUT
         31                     C          G,E         ; GZAI, EATA COORDINATE WHERE CALCULATION DONE
         32                     C          XE (I)      ; X-COORDINATE OF NODES CONSTITUTING THE ELEMENT
         33                     C          YE (I)      ; Y-COORDINATE OF NODES CONSTITUTING THE ELEMENT
         34                     C          N           ; NUMBER OF NODES ASSIGNED TO ONE ELEMENT
         35                     C
         36                     C       (2) OUTPUT
         37                     C          DNX(I)      ; X-DERIVERTIVES OF SHAPE FUNCTION N(I)
         38                     C          DNI(I)      ; Y-DERIVERTIVES OF SHAPE FUNCTION N(I)
         39                     C
         40                     C
         41                     C
         42                     C
         43     1                     DO 10 I = 1 , N
         44     1                           DNG(I) = SHAPEG(I,G,E,2)
         45     1                           DNE(I) = SHAPEG(I,G,E,3)
         46     1                  10 CONTINUE
         47                     C
         48                           DXG = DNG(1)*XE(1)+DNG(2)*XE(2)+DNG(3)*XE(3)+DNG(4)*XE(4)
         49                           DYG = DNG(1)*YE(1)+DNG(2)*YE(2)+DNG(3)*YE(3)+DNG(4)*YE(4)
         50                           DXE = DNE(1)*XE(1)+DNE(2)*XE(2)+DNE(3)*XE(3)+DNE(4)*XE(4)
         51                           DYE = DNE(1)*YE(1)+DNE(2)*YE(2)+DNE(3)*YE(3)+DNE(4)*YE(4)
         52                     C
         53                           DET = DXG*DYE-DYG*DXE
         54                     C
                              <<< Loop-information Start >>>
                              <<<  [PARALLELIZATION]
                              <<<    Standard iteration count: 640
                              <<<  [OPTIMIZATION]
                              <<<    SIMD(VL: 16)
                              <<< Loop-information  End >>>
         55     1  pp    v            DO 20 I = 1 , N
         56     1   p    v                DNX(I) = ( DYE*DNG(I)-DYG*DNE(I))/DET
         57     1   p    v                DNY(I) = (-DXE*DNG(I)+DXG*DNE(I))/DET
         58     1   p    v         20 CONTINUE
         59                     C
         60                     C
         61                           RETURN
         62                           END

