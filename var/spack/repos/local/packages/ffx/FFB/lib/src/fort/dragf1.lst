         13                           SUBROUTINE DRAGF1(VS,A,U,V,P,DSB,ASB,ACB,DNXB,DNYB,LEB,NODE,NEB,N,
         14                          *                  CDN,CDS,CD,CLN,CLS,CL)
         15                           IMPLICIT REAL*8(A-H,O-Z)
         16                           DIMENSION U(*),V(*),P(*),
         17                          1          DSB(NEB),ASB(NEB),ACB(NEB),DNXB(N,NEB),DNYB(N,NEB),
         18                          2          LEB(NEB),NODE(N,*)
         19                     C
         20                     C
         21                     C      CALCULATE DRAG & LIFT COEFFICIENT OF A BODY ; DRAG CALCULATION 1
         22                     C         ( 2-D CALCULATION )
         23                     C
         24                     C
         25                     C     ARGUMENT LISTINGS
         26                     C       (1) INPUT
         27                     C          VS          ; VISCOSITY
         28                     C          A           ; FRONTAL PROJECTIVE AREA OF THE BODY
         29                     C          U (IP)      ; X-DIR. VELOCITY COMPONENT DEFINED AT NODE
         30                     C          V (IP)      ; Y-DIR. VELOCITY COMPONENT DEFINED AT NODE
         31                     C          P (IE)      ; PRESSURE                  DEFINED AT ELEMENT
         32                     C          DSB   (IB)  ; LENGTH      OF BOUNDARY SIDE
         33                     C          ASB   (IB)  ; ANGLE BETWEEN THE SIDE AND X-AXIS (   SINE )
         34                     C          ACB   (IB)  ; ANGLE BETWEEN THE SIDE AND X-AXIS ( COSINE )
         35                     C          DNXB(I,IB)  ; X-DIRIVERTIVE  AT THE SIDE'S CENTER POINT
         36                     C          DNYB(I,IB)  ; Y-DIRIVERTIVE  AT THE SIDE'S CENTER POINT
         37                     C          LEB   (IB)  ; ELEMENT NO.        FACING ON THE BODY
         38                     C          NODE(I,IE)  ; NODE NUMBER TABLE
         39                     C          NEB         ; NUMBER OF ELEMENTS FACING ON THE BODY
         40                     C          N           ; NUMBER OF NODE ASSIGNED TO ONE ELEMENT
         41                     C
         42                     C       (2) OUTPUT
         43                     C          CDN         ; DRAG COEFFICIENT CAUSED BY NORMAL   STRESS
         44                     C          CDS         ; DRAG COEFFICIENT CAUSED BY SHEARING STRESS
         45                     C          CD          ; DRAG COEFFICIENT CAUSED BY TOTAL    STRESS
         46                     C          CLN         ; LIFT COEFFICIENT CAUSED BY NORMAL   STRESS
         47                     C          CLS         ; LIFT COEFFICIENT CAUSED BY SHEARING STRESS
         48                     C          CL          ; LIFT COEFFICIENT CAUSED BY TOTAL    STRESS
         49                     C
         50                     C
         51                           FSX = 0.D0
         52                           FSY = 0.D0
         53                           FNX = 0.D0
         54                           FNY = 0.D0
         55                     C
                              <<< Loop-information Start >>>
                              <<<  [PARALLELIZATION]
                              <<<    Standard iteration count: 94
                              <<<  [OPTIMIZATION]
                              <<<    SIMD(VL: 8)
                              <<< Loop-information  End >>>
         56     1  pp    v            DO 100 IB = 1 , NEB
         57     1   p    v                A1 = U(NODE(1,LEB(IB)))*ACB(IB)+V(NODE(1,LEB(IB)))*ASB(IB)
         58     1   p    v                A2 = U(NODE(2,LEB(IB)))*ACB(IB)+V(NODE(2,LEB(IB)))*ASB(IB)
         59     1   p    v                A3 = U(NODE(3,LEB(IB)))*ACB(IB)+V(NODE(3,LEB(IB)))*ASB(IB)
         60     1   p    v                A4 = U(NODE(4,LEB(IB)))*ACB(IB)+V(NODE(4,LEB(IB)))*ASB(IB)
         61     1   p    v                B1 =-U(NODE(1,LEB(IB)))*ASB(IB)+V(NODE(1,LEB(IB)))*ACB(IB)
         62     1   p    v                B2 =-U(NODE(2,LEB(IB)))*ASB(IB)+V(NODE(2,LEB(IB)))*ACB(IB)
         63     1   p    v                B3 =-U(NODE(3,LEB(IB)))*ASB(IB)+V(NODE(3,LEB(IB)))*ACB(IB)
         64     1   p    v                B4 =-U(NODE(4,LEB(IB)))*ASB(IB)+V(NODE(4,LEB(IB)))*ACB(IB)
         65     1               C
         66     1   p    v                USX = DNXB(1,IB)*A1+DNXB(2,IB)*A2+DNXB(3,IB)*A3+DNXB(4,IB)*A4
         67     1   p    v                USY = DNYB(1,IB)*A1+DNYB(2,IB)*A2+DNYB(3,IB)*A3+DNYB(4,IB)*A4
         68     1   p    v                UNX = DNXB(1,IB)*B1+DNXB(2,IB)*B2+DNXB(3,IB)*B3+DNXB(4,IB)*B4
         69     1   p    v                UNY = DNYB(1,IB)*B1+DNYB(2,IB)*B2+DNYB(3,IB)*B3+DNYB(4,IB)*B4
         70     1               C
         71     1   p    v                USN =-USX*ASB(IB)+USY*ACB(IB)
         72     1   p    v                UNS = UNX*ACB(IB)+UNY*ASB(IB)
         73     1   p    v                UNN =-UNX*ASB(IB)+UNY*ACB(IB)
         74     1               C
         75     1   p    v                FS  = DSB(IB)*(VS*(USN+UNS))
         76     1   p    v                FN  = DSB(IB)*(VS*(UNN+UNN)-P(LEB(IB)))
         77     1               C
         78     1   p    v                FSX = FSX+FS*ACB(IB)
         79     1   p    v                FSY = FSY+FS*ASB(IB)
         80     1   p    v                FNX = FNX-FN*ASB(IB)
         81     1   p    v                FNY = FNY+FN*ACB(IB)
         82     1   p    v        100 CONTINUE
         83                     C
         84                           CDS = FSX/(0.5D0*A)
         85                           CLS = FSY/(0.5D0*A)
         86                           CDN = FNX/(0.5D0*A)
         87                           CLN = FNY/(0.5D0*A)
         88                           CD  = CDS+CDN
         89                           CL  = CLS+CLN
         90                     C
         91                     C
         92                           RETURN
         93                           END

