         13                           SUBROUTINE DRAGF0(X,Y,NODE,N,LEB,LESB,NEB,
         14                          *                  DSB,ASB,ACB,DNXB,DNYB,DXG,DXE,DYG,DYE,DET)
         15                           IMPLICIT REAL*8(A-H,O-Z)
         16                           DIMENSION X(*),Y(*),NODE(N,*),LEB(NEB),LESB(NEB),
         17                          1          DSB(NEB),ASB(NEB),ACB(NEB),DNXB(N,NEB),DNYB(N,NEB),
         18                          2          DXG(NEB),DXE(NEB),DYG(NEB),DYE(NEB),DET(NEB)
         19                     C
         20                           DIMENSION G(4),E(4),DNG(4,4),DNE(4,4)
         21                           DATA      G /  0.D0 ,  1.D0 ,  0.D0 , -1.D0 /
         22                           DATA      E / -1.D0 ,  0.D0 ,  1.D0 ,  0.D0 /
         23                     C
         24                     C
         25                     C      CALCULATE BOUNDARY ELEMENT CONSTANTS ; DRAG CALCULATION 0
         26                     C         ( 2-D CALCULATION )
         27                     C
         28                     C
         29                     C     ARGUMENT LISTINGS
         30                     C       (1) INPUT
         31                     C          X     (IP)  ; X-COORDINATE OF NODE ' IP '
         32                     C          Y     (IP)  ; Y-COORDINATE OF NODE ' IP '
         33                     C          NODE(I,IE)  ; NODE NO. TABLE BASED ON ELEMENT
         34                     C          N           ; NUMBER OF NODES ASSIGNED TO ONE ELEMENT
         35                     C          LEB   (IB)  ; ELEMENT  SPECIFYING LIST VECTOR
         36                     C          LESB  (IB)  ; SIDE NO. SPECIFYING LIST VECTOR
         37                     C          NEB         ; NUMBER OF THE SIDES
         38                     C
         39                     C       (2) OUTPUT
         40                     C          DSB   (IB)  ; LENGTH      OF BOUNDARY SIDE
         41                     C          ASB   (IB)  ; ANGLE BETWEEN THE SIDE AND X-AXIS (   SINE )
         42                     C          ACB   (IB)  ; ANGLE BETWEEN THE SIDE AND X-AXIS ( COSINE )
         43                     C          DNXB(I,IB)  ; X-DIRIVERTIVE  AT THE SIDE'S CENTER POINT
         44                     C          DNYB(I,IB)  ; Y-DIRIVERTIVE  AT THE SIDE'S CENTER POINT
         45                     C
         46                     C       (3) WORK
         47                     C          DET   (IB)  ; DETERMINANT OF JACOBIAN MATRIX
         48                     C          DXG   (IB)  ; G-DIR. DERIVERTIVE OF X
         49                     C          DXE   (IB)  ; E-DIR. DERIVERTIVE OF X
         50                     C          DYG   (IB)  ; G-DIR. DERIVERTIVE OF Y
         51                     C          DYE   (IB)  ; E-DIR. DERIVERTIVE OF Y
         52                     C
         53                     C      (1) CALCULATE  DSB(IB) , ASB(IB) , ACB(IB)
         54                     C
                              <<< Loop-information Start >>>
                              <<<  [PARALLELIZATION]
                              <<<    Standard iteration count: 20
                              <<<  [OPTIMIZATION]
                              <<<    SIMD(VL: 8)
                              <<<    PREFETCH(HARD) Expected by compiler :
                              <<<      LEB, LESB, DSB, ACB, ASB
                              <<< Loop-information  End >>>
         55     1  pp    v            DO 100 IB = 1 , NEB
         56     1   p    v                IE = LEB (IB)
         57     1   p    v                IS = LESB(IB)
         58     1   p    v                X1 = X(NODE(MOD(IS  ,N)+1,IE))-X(NODE(IS,IE))
         59     1   p    v                Y1 = Y(NODE(MOD(IS  ,N)+1,IE))-Y(NODE(IS,IE))
         60     1   p    v                X2 = X(NODE(MOD(IS+1,N)+1,IE))-X(NODE(IS,IE))
         61     1   p    v                Y2 = Y(NODE(MOD(IS+1,N)+1,IE))-Y(NODE(IS,IE))
         62     1               
         63     1   p    v                DSB(IB) = DSQRT(X1*X1+Y1*Y1)
         64     1   p    v                ANG     = DATAN2(Y1,X1)
         65     1   p    v                ASB(IB) = DSIN(ANG)
         66     1   p    v                ACB(IB) = DCOS(ANG)
         67     1   p    v                DIRECT  = -X2*ASB(IB)+Y2*ACB(IB)
         68     2   p    v                IF(DIRECT.LT.0.D0) THEN
         69     2   p    v                    ASB(IB) = -ASB(IB)
         70     2   p    v                    ACB(IB) = -ACB(IB)
         71     2   p    v                ENDIF
         72     1   p    v        100 CONTINUE
         73                     C
         74                     C      (2) CALCULATE  DNXB(IB) DNYB(IB)
         75                     C
         76                     C          SETTING OF  G , E  DERIVERTIVES OF SHAPE FUNCTION
         77                     C
                              <<< Loop-information Start >>>
                              <<<  [OPTIMIZATION]
                              <<<    PREFETCH(HARD) Expected by compiler :
                              <<<      (unknown)
                              <<< Loop-information  End >>>
         78     1   s    s            DO 110 IS = 1 , N
         79     1   s    s                DNG(1,IS) = SHAPE2(1,G(IS),E(IS),2)
         80     1   s    s                DNG(2,IS) = SHAPE2(2,G(IS),E(IS),2)
         81     1   s    s                DNG(3,IS) = SHAPE2(3,G(IS),E(IS),2)
         82     1   s    s                DNG(4,IS) = SHAPE2(4,G(IS),E(IS),2)
         83     1               C
         84     1   s    s                DNE(1,IS) = SHAPE2(1,G(IS),E(IS),3)
         85     1   s    s                DNE(2,IS) = SHAPE2(2,G(IS),E(IS),3)
         86     1   s    s                DNE(3,IS) = SHAPE2(3,G(IS),E(IS),3)
         87     1   s    s                DNE(4,IS) = SHAPE2(4,G(IS),E(IS),3)
         88     1   s    s        110 CONTINUE
         89                     C
         90                     C          CALCULATION  OF  G , E  DERIVERTIVES OF X , Y
         91                     C
                              <<< Loop-information Start >>>
                              <<<  [PARALLELIZATION]
                              <<<    Standard iteration count: 117
                              <<<  [OPTIMIZATION]
                              <<<    FUSED(lines: 92,108)
                              <<<    SIMD(VL: 8)
                              <<<    SOFTWARE PIPELINING(IPC: 1.30, ITR: 32, MVE: 2, POL: S)
                              <<<    PREFETCH(HARD) Expected by compiler :
                              <<<      LEB, LESB, DXE, DXG, DYE, DET, DYG
                              <<< Loop-information  End >>>
         92     1  pp    v            DO 200 IB = 1 , NEB
         93     1   p    v                IE  = LEB(IB)
         94     1   p    v                IS = LESB(IB)
         95     1   p    v                DXG(IB) = X(NODE(1,IE))*DNG(1,IS)+X(NODE(2,IE))*DNG(2,IS)
         96     1                    &             +X(NODE(3,IE))*DNG(3,IS)+X(NODE(4,IE))*DNG(4,IS)
         97     1   p    v                DYG(IB) = Y(NODE(1,IE))*DNG(1,IS)+Y(NODE(2,IE))*DNG(2,IS)
         98     1                    &             +Y(NODE(3,IE))*DNG(3,IS)+Y(NODE(4,IE))*DNG(4,IS)
         99     1               C
        100     1   p    v                DXE(IB) = X(NODE(1,IE))*DNE(1,IS)+X(NODE(2,IE))*DNE(2,IS)
        101     1                    &             +X(NODE(3,IE))*DNE(3,IS)+X(NODE(4,IE))*DNE(4,IS)
        102     1   p    v                DYE(IB) = Y(NODE(1,IE))*DNE(1,IS)+Y(NODE(2,IE))*DNE(2,IS)
        103     1                    &             +Y(NODE(3,IE))*DNE(3,IS)+Y(NODE(4,IE))*DNE(4,IS)
        104     1   p    v        200 CONTINUE
        105                     C
        106                     C          CALCULATION OF DETERMINANT OF JACOBIAN MATRIX
        107                     C
                              <<< Loop-information Start >>>
                              <<<  [OPTIMIZATION]
                              <<<    FUSED
                              <<< Loop-information  End >>>
        108     1                     DO 300 IB = 1 , NEB
        109     1                         DET(IB) = DXG(IB)*DYE(IB)-DYG(IB)*DXE(IB)
        110     1                 300 CONTINUE
        111                     C
        112                     C          CALCULATION OF X , Y DERIVERTIVES OF SHAPE FUNCTION
        113                     C
                              <<< Loop-information Start >>>
                              <<<  [PARALLELIZATION]
                              <<<    Standard iteration count: 135
                              <<<  [OPTIMIZATION]
                              <<<    SIMD(VL: 8)
                              <<<    SOFTWARE PIPELINING(IPC: 2.35, ITR: 32, MVE: 2, POL: S)
                              <<<    PREFETCH(HARD) Expected by compiler :
                              <<<      DXG, LESB, DET, DYE, DXE, DYG
                              <<< Loop-information  End >>>
        114     1  pp    v            DO 400 IB = 1 , NEB
        115     1   p    v                IS = LESB(IB)
        116     1   p    v                DNXB(1,IB) =( DYE(IB)*DNG(1,IS)-DYG(IB)*DNE(1,IS))/DET(IB)
        117     1   p    v                DNXB(2,IB) =( DYE(IB)*DNG(2,IS)-DYG(IB)*DNE(2,IS))/DET(IB)
        118     1   p    v                DNXB(3,IB) =( DYE(IB)*DNG(3,IS)-DYG(IB)*DNE(3,IS))/DET(IB)
        119     1   p    v                DNXB(4,IB) =( DYE(IB)*DNG(4,IS)-DYG(IB)*DNE(4,IS))/DET(IB)
        120     1               C
        121     1   p    v                DNYB(1,IB) =(-DXE(IB)*DNG(1,IS)+DXG(IB)*DNE(1,IS))/DET(IB)
        122     1   p    v                DNYB(2,IB) =(-DXE(IB)*DNG(2,IS)+DXG(IB)*DNE(2,IS))/DET(IB)
        123     1   p    v                DNYB(3,IB) =(-DXE(IB)*DNG(3,IS)+DXG(IB)*DNE(3,IS))/DET(IB)
        124     1   p    v                DNYB(4,IB) =(-DXE(IB)*DNG(4,IS)+DXG(IB)*DNE(4,IS))/DET(IB)
        125     1   p    v        400 CONTINUE
        126                     C
        127                     C
        128                           RETURN
        129                           END

