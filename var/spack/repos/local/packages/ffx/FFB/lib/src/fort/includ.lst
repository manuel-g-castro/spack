         13                           SUBROUTINE INCLUD(IE,X,Y,NODE,NE,NP,N,XP,YP,IRN)
         14                           IMPLICIT REAL*4 (A-H,O-Z)
         15                           DIMENSION X(NP),Y(NP),NODE(N,NE)
         16                           DATA DELT / 1.E-20 /
         17                     C
         18                     C
         19                     C      JUDGE IF SPECIFIED POINT IS INCLUDED IN THE SPECIFIED ELEMENT
         20                     C         ( 2-D GRAPHICS )
         21                     C
         22                     C
         23                     C     ARGUMENT LISTINGS
         24                     C       (1) INPUT
         25                     C          IE          ; SPECIFIED ELEMENT NO.
         26                     C          X     (IP)  ; X-DIR. COORDINATE         OF NODE
         27                     C          Y     (IP)  ; Y-DIR. COORDINATE         OF NODE
         28                     C          NODE(I,IE)  ; NODE NUMBER TABLE
         29                     C          NE          ; NUMBER OF TOTAL ELEMENTS
         30                     C          NP          ; NUMBER OF TOTAL NODES
         31                     C          N           ; NUMBER OF NODES ASSIGNED TO ONE ELEMENT
         32                     C          XP          ; X-DIR. COORDINATE OF SPECIFIED POINT
         33                     C          YP          ; Y-DIR. COORDINATE OF SPECIFIED POINT
         34                     C
         35                     C       (2) OUTPUT
         36                     C          IRN         ; RETURN CODE TO REPORT RESULT
         37                     C                   0 --- NOT INCLUDED
         38                     C                   1 ---     INCLUDED
         39                     C
         40                     C
         41                           IRN = 0
         42                     C
                              <<< Loop-information Start >>>
                              <<<  [OPTIMIZATION]
                              <<<    FULL UNROLLING
                              <<< Loop-information  End >>>
         43     1       f             DO 100 IF = 1 , 2
         44     1       f                 X1  = X(NODE(IF+1,IE))-X(NODE(1,IE))
         45     1       f                 Y1  = Y(NODE(IF+1,IE))-Y(NODE(1,IE))
         46     1       f                 X2  = X(NODE(IF+2,IE))-X(NODE(1,IE))
         47     1       f                 Y2  = Y(NODE(IF+2,IE))-Y(NODE(1,IE))
         48     1       f                 DET = X1*Y2-X2*Y1
         49     1               C
         50     1       f                 XF  = XP              -X(NODE(1,IE))
         51     1       f                 YF  = YP              -Y(NODE(1,IE))
         52     1               C
         53     1       f                 A   = ( Y2*XF-X2*YF)/DET
         54     1       f                 B   = (-Y1*XF+X1*YF)/DET
         55     1               C
         56     1       f                 IF(A       .LT.    -DELT) GO TO 100
         57     1       f                 IF(B       .LT.    -DELT) GO TO 100
         58     1       f                 IF(A+B     .GT. 1.0+DELT) GO TO 100
         59     1       f                 IRN = 1
         60     1       f                 RETURN
