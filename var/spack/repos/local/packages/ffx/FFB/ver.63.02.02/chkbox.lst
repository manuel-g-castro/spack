          1                           SUBROUTINE CHKBOX(DOM1,DOM2,ICHECK)
          2                           IMPLICIT NONE
          3                     C
          4                     C[INPUT]
          5                           REAL*4    DOM1(6),DOM2(6)
          6                     C[OUTPUT]
          7                           INTEGER*4 ICHECK
          8                     C     ICHECK : 0 TWO BOUNDING BOXES ARE NOT OVERLAPPED
          9                     C     ICHECK : 1 TWO BOUNDING BOXES ARE     OVERLAPPED
         10                     C[LOCAL]
         11                           REAL*4    XMIN1,XMAX1,DX1
         12                           REAL*4    XMIN2,XMAX2,DX2
         13                           REAL*4    YMIN1,YMAX1,DY1
         14                           REAL*4    YMIN2,YMAX2,DY2
         15                           REAL*4    ZMIN1,ZMAX1,DZ1
         16                           REAL*4    ZMIN2,ZMAX2,DZ2
         17                     C
         18                           REAL*4   EPS
         19                           DATA EPS  /1.0E-1/
         20                     C
         21                           INTEGER*4 ICHKX,ICHKY,ICHKZ
         22                     C
         23                     CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
         24                     C
         25                           XMIN1=DOM1(1)
         26                           XMIN2=DOM2(1)
         27                           YMIN1=DOM1(2)
         28                           YMIN2=DOM2(2)
         29                           ZMIN1=DOM1(3)
         30                           ZMIN2=DOM2(3)
         31                           XMAX1=DOM1(4)
         32                           XMAX2=DOM2(4)
         33                           YMAX1=DOM1(5)
         34                           YMAX2=DOM2(5)
         35                           ZMAX1=DOM1(6)
         36                           ZMAX2=DOM2(6)
         37                     C
         38                           DX1=XMAX1-XMIN1
         39                           DX2=XMAX2-XMIN2
         40                           DY1=YMAX1-YMIN1
         41                           DY2=YMAX2-YMIN2
         42                           DZ1=ZMAX1-ZMIN1
         43                           DZ2=ZMAX2-ZMIN2
         44                     C
         45                           XMIN1=XMIN1-DX1*EPS
         46                           XMIN2=XMIN2-DX2*EPS
         47                           YMIN1=YMIN1-DY1*EPS
         48                           YMIN2=YMIN2-DY2*EPS
         49                           ZMIN1=ZMIN1-DZ1*EPS
         50                           ZMIN2=ZMIN2-DZ2*EPS
         51                     C
         52                           XMAX1=XMAX1+DX1*EPS
         53                           XMAX2=XMAX2+DX2*EPS
         54                           YMAX1=YMAX1+DY1*EPS
         55                           YMAX2=YMAX2+DY2*EPS
         56                           ZMAX1=ZMAX1+DZ1*EPS
         57                           ZMAX2=ZMAX2+DZ2*EPS
         58                     C
         59                           ICHKX=1
         60                           IF(XMIN1.GT.XMAX2) ICHKX=0
         61                           IF(XMIN2.GT.XMAX1) ICHKX=0
         62                     C
         63                           ICHKY=1
         64                           IF(YMIN1.GT.YMAX2) ICHKY=0
         65                           IF(YMIN2.GT.YMAX1) ICHKY=0
         66                     C
         67                           ICHKZ=1
         68                           IF(ZMIN1.GT.ZMAX2) ICHKZ=0
         69                           IF(ZMIN2.GT.ZMAX1) ICHKZ=0
         70                     C
         71                           ICHECK=ICHKX*ICHKY*ICHKZ
         72                     C
         73                           RETURN
         74                           END
