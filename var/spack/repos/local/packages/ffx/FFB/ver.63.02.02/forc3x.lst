         60                     C          NODE  (I,IE); NODE NO. TABLE BASED ON ELEMENT
         61                     C          NE          ; NUMBER OF TOTAL     ELEMENTS
         62                     C          NP          ; NUMBER OF TOTAL     NODES
         63                     C          N           ; NUMBER OF NODES ASSIGNED TO ONE ELEMENT
         64                     C          A       (IB); AREA                             OF SURFACE IB
         65                     C          XNB     (IB); X COMPONENT OF THE NORMAL VECTOR OF SURFACE IB
         66                     C          YNB     (IB); Y COMPONENT OF THE NORMAL VECTOR OF SURFACE IB
         67                     C          ZNB     (IB); X COMPONENT OF THE NORMAL VECTOR OF SURFACE IB
         68                     C          DNXB (I,IB) ; INTEGRATED X DERIVATIVE OF SHAPE FUNCTION
         69                     C          DNYB (I,IB) ; INTEGRATED Y DERIVATIVE OF SHAPE FUNCTION
         70                     C          DNZB (I,IB) ; INTEGRATED Z DERIVATIVE OF SHAPE FUNCTION
         71                     C          LEB  (I,IB) ; ELEMENT(1, & SURFACE(2, NUMBER SPECIFYING LIST
         72                     C          NB          ; NUMBER OF SURFACES
         73                     C
         74                     C       (2) OUTPUT
         75                     C          FX           ; X COMPONENT OF FLUID FORCE ACTING ON THE BODY
         76                     C          FY           ; Y COMPONENT OF FLUID FORCE ACTING ON THE BODY
         77                     C          FZ           ; Z COMPONENT OF FLUID FORCE ACTING ON THE BODY
         78                     C
         79                     C
                              <<< Loop-information Start >>>
