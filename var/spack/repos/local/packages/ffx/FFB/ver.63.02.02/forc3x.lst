        128     4                    &              *(XNB(IB)*V(IP)*DNXB(I,IB)+XNB(IB)*U(IP)*DNYB(I,IB)
        129     4                    &               +YNB(IB)*V(IP)*DNYB(I,IB)+YNB(IB)*V(IP)*DNYB(I,IB)
        130     4                    &               +ZNB(IB)*V(IP)*DNZB(I,IB)+ZNB(IB)*W(IP)*DNYB(I,IB))
        131     4   p    s                       FZBUF=VISC(IE)
        132     4                    &              *(XNB(IB)*W(IP)*DNXB(I,IB)+XNB(IB)*U(IP)*DNZB(I,IB)
        133     4                    &               +YNB(IB)*W(IP)*DNYB(I,IB)+YNB(IB)*V(IP)*DNZB(I,IB)
        134     4                    &               +ZNB(IB)*W(IP)*DNZB(I,IB)+ZNB(IB)*W(IP)*DNZB(I,IB))
        135     4               C
        136     4   p    s                       FX=FX+FXBUF
        137     4   p    s                       FY=FY+FYBUF
        138     4   p    s                       FZ=FZ+FZBUF
        139     4               C
        140     4   p    s                       FXVIS(IB)=FXVIS(IB)+FXBUF
        141     4   p    s                       FYVIS(IB)=FYVIS(IB)+FYBUF
        142     4   p    s                       FZVIS(IB)=FZVIS(IB)+FZBUF
        143     4   p                         END IF
