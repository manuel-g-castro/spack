         15                           SUBROUTINE FORC3X(IMODE,U,V,W,P,VISC,NODE,NE,NP,N1,N2,
         16                          *                  A,XNB,YNB,ZNB,DNXB,DNYB,DNZB,LEB,NB,
         17                          *                  FX,FY,FZ,IVOF,NEFLD2,LEFLD2,LEFIX,
         18                          *                  FXVIS,FYVIS,FZVIS)
         19                     C
         20                           IMPLICIT NONE
         21                     C
         22                           INTEGER*4 IMODE,NODE,NE,NP,N1,N2,LEB,NB
         23                           REAL*4    U,V,W,P,VISC,
         24                          *          A,XNB,YNB,ZNB,DNXB,DNYB,DNZB,
         25                          *          FX,FY,FZ,FXVIS,FYVIS,FZVIS
         26                     C
         27                           INTEGER*4 IE,IB,I,IP
